{
  "definitions": {
    "Ticker": {
      "type": "object",
      "properties": {
        "ask": {
          "format": "double",
          "type": "number"
        },
        "bid": {
          "format": "double",
          "type": "number"
        }
      },
      "required": [ "ask", "bid" ]
    },
    "TickerDetailed": {
      "type": "object",
      "properties": {
        "ask": {
          "format": "double",
          "type": "number"
        },
        "bid": {
          "format": "double",
          "type": "number"
        },
        "last": {
          "format": "double",
          "type": "number"
        },
        "vol": {
          "format": "double",
          "type": "number"
        }
      },
      "required": [ "ask", "bid", "last", "double" ]
    },
    "OrderBook": {
      "type": "object",
      "properties": {
        "asks": {
          "type": "array",
          "items": {
            "type": "array",
            "items": {
              "format": "double",
              "type": "number"
            }
          }
        },
        "bids": {
          "type": "array",
          "items": {
            "type": "array",
            "items": {
              "format": "double",
              "type": "number"
            }
          }
        }
      }
    },
    "Trades": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "amount": {
            "format": "double",
            "type": "number"
          },
          "date": {
            "format": "epochtime",
            "type": "number"
          },
          "price": {
            "format": "double",
            "type": "number"
          },
          "tid": {
            "type": "number"
          }
        }
      }
    },
    "ErrorMessage": {
      "type": "object",
      "properties": {
        "error": {
          "type": "string"
        }
      },
      "required": []
    },
    "AccountInfo": {
      "type": "object",
      "properties": {
        "accNo": {
          "type": "number"
        },
        "btcBal": {
          "format": "double",
          "type": "number"
        },
        "btcDeposit": {
          "pattern": "^[13][a-km-zA-HJ-NP-Z1-9]{25,34}$",
          "type": "string"
        },
        "email": {
          "pattern": "([\\w-\\.]+)@((?:[\\w]+\\.)+)([a-zA-Z]{2,4})",
          "type": "string"
        },
        "error": {
          "type": "number"
        },
        "sgdBal": {
          "format": "double",
          "type": "number"
        }
      },
      "required": []
    },
    "PendingOrders": {
      "type": "object",
      "properties": {
        "error": {
          "type": "number"
        },
        "orders": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "date": {
                "format": "epochtime",
                "type": "number"
              },
              "price": {
                "format": "double",
                "type": "number"
              },
              "qty": {
                "format": "double",
                "type": "number"
              },
              "ticket": {
                "type": "number"
              },
              "type": {
                "type": "string"
              }
            }
          }
        }
      }
    },
    "OrderHistory": {
      "type": "object",
      "properties": {
        "error": {
          "type": "number"
        },
        "orders": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "date_created": {
                "format": "epochtime",
                "type": "number"
              },
              "date_executed": {
                "format": "epochtime",
                "type": "number"
              },
              "price": {
                "type": "string"
              },
              "qty": {
                "type": "string"
              },
              "status": {
                "type": "string"
              },
              "ticket": {
                "type": "number"
              },
              "type": {
                "type": "string"
              }
            }
          }
        }
      }
    },
    "OrderStatus": {
      "type": "object",
      "properties": {
        "error": {
          "type": "number"
        },
        "msg": {
          "type": "string"
        },
        "pending_oid": {
          "type": "string"
        }
      },
      "required": []
    },
    "ErrorNumber": {
      "type": "object",
      "properties": {
        "error": {
          "type": "number"
        }
      },
      "required": []
    },
    "ErrorNumberAndMessage": {
      "type": "object",
      "properties": {
        "error": {
          "type": "number"
        },
        "msg": {
          "type": "string"
        }
      },
      "required": []
    },
    "TradesParams": {
      "type": "object",
      "properties": {
        "since": {
          "description": "Trade ID to begin trade history from. You should cache trade history and query only new trades by passing in last known trade id",
          "type": "number"
        }
      },
      "required": []
    },
    "TimestampParams": {
      "type": "object",
      "properties": {
        "timestamp": {
          "description": "Current Unix timestamp",
          "format": "epochtime",
          "type": "number"
        }
      },
      "required": [ "timestamp" ]
    },
    "OrderHistoryParams": {
      "type": "object",
      "properties": {
        "timestamp": {
          "description": "Current Unix timestamp",
          "format": "epochtime",
          "type": "number"
        },
        "limit": {
          "type": "number"
        }
      },
      "required": [ "timestamp", "limit" ]
    },
    "OrderParams": {
      "type": "object",
      "properties": {
        "timestamp": {
          "description": "Current Unix timestamp",
          "format": "epochtime",
          "type": "number"
        },
        "qty": {
          "description": "Quantity of bitcoins",
          "format": "double",
          "type": "number"
        },
        "price": {
          "description": "Price to place order at",
          "format": "double",
          "type": "number"
        },
        "type": {
          "pattern": "B|S",
          "format": "char",
          "description": "Whether it is a buy or sell order. Must be either 'B' or 'S' only.",
          "type": "string"
        }
      },
      "required": [ "timestamp", "qty", "price", "type" ]
    },
    "CancelOrderParams": {
      "type": "object",
      "properties": {
        "timestamp": {
          "description": "Current Unix timestamp",
          "format": "epochtime",
          "type": "number"
        },
        "orderNo": {
          "description": "Ticket Number of Pending Order to cancel",
          "type": "number"
        }
      },
      "required": [ "timestamp", "orderNo" ]
    },
    "WithdrawParams": {
      "type": "object",
      "properties": {
        "timestamp": {
          "description": "Current Unix timestamp",
          "format": "epochtime",
          "type": "number"
        },
        "amount": {
          "description": "Amount of bitcoins/dollars to withdraw",
          "format": "double",
          "type": "number"
        },
        "destination": {
          "description": "Bitcoin address to withdraw to, leave blank for XFERS",
          "pattern": "^[13][a-km-zA-HJ-NP-Z1-9]{25,34}$",
          "type": "string"
        },
        "type": {
          "pattern": "BTC|XFERS",
          "description": "BTC/XFERS (XFERS only for FYB-SG)",
          "type": "string"
        }
      },
      "required": [ "timestamp", "amount", "type" ]
    }
  },
